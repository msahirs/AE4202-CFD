FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 3.9592226;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            calculated;
        value           uniform 3.9592226;
    }
    Airfoil
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            zeroGradient;
    }
    Flap
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           nonuniform List<scalar> 0();
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 3.9592226;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 3.9592226;
    }
}


