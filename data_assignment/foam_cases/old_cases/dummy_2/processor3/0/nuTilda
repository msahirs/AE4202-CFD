FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 3.9592226e-07;

boundaryField
{
    Side
    {
        type            empty;
    }
    Inlet
    {
        type            inletOutlet;
        inletValue      uniform 3.9592226e-07;
        value           uniform 3.9592226e-07;
    }
    Airfoil
    {
        type            nutUSpaldingWallFunction;
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            zeroGradient;
    }
    Flap
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 3.9592226e-07;
    }
    procBoundary3to4
    {
        type            processor;
        value           uniform 3.9592226e-07;
    }
}


